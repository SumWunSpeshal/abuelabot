{
  "trailingComma": "none",
  "tabWidth": 2,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "strict",
  "printWidth": 120,
  "quoteProps": "as-needed",
  "singleQuote": true,
  "semi": true,
  "overrides": [
    {
      "files": "package.json",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
